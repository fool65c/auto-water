[Unit]
Description=Auto Water service for Leonas plant bed
After=network.target

[Service]
Type=simple
ExecStart=/home/kmager/auto-water/.auto-water/bin/python /home/kmager/auto-water/auto-water.py /home/kmager/auto-water/config/leona.yaml
User=kmager
Group=kmager
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target

